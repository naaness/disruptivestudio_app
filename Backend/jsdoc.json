{
  "plugins": ["jsdoc-http-plugin"],
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs|src/tests)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": "true",
    "destination": "./docs",
    "template": "node_modules/docdash",
    "readme": "../Readme.md"
  }
}